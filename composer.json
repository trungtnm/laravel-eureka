{
    "name": "trungtnm/laravel-eureka",
    "description": "Eureka starter pack for Laravel",
    "keywords": [
        "wjezowski",
        "laravel-eureka",
        "laravel",
        "php",
        "eureka"
    ],
    "homepage": "https://github.com/trungtnm/laravel-eureka",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-pcntl": "*",
        "ext-dom": "*",
        "piwvh/php-eureka": "dev-master",
        "illuminate/console": "^v10.10",
        "illuminate/http": "^v10.10",
        "illuminate/routing": "v10.10",
        "illuminate/support": "^v10.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Wjezowski\\LaravelEureka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wjezowski\\LaravelEureka\\Tests\\": "tests/"
        }
    },
